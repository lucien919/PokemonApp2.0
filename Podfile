# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'PokemonApp2.0' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PokemonApp2.0
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'SwiftKeychainWrapper'
  

  target 'PokemonApp2.0Tests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
  end

  target 'PokemonApp2.0UITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
  end

end
